Program Test;
{ 这是一个计算从 0 到 100 累加求和的程序 }

Var
    n   : Integer;
    i   : Integer;
    sum : Integer;
    a, b: Integer;
    f   : Integer;
    ff  : Real;

Begin
    Input n;
    i   := 0;
    sum := 0;

    While i <= n Do
    Begin
        sum := sum + i;
        i   := i + 1
    End;

    a   := 2;
    b   := 200;
    f   := -1;
    If a = b Then f := 1
    Else f := 0;

    Print sum;
    Print f
End.


